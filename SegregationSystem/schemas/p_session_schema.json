{
  "type": "object",
  "properties": {
    "uuid": {"type": "string"},
    "features": {
      "type": "object",
      "properties": {
        "alpha": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "beta": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "delta": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "theta": {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "environment": {
          "type": "string",
          "enum": [
            "indoor",
            "outdoor"
          ]
        }
      },
      "required": ["alpha", "beta", "delta", "theta", "environment"]
    },
    "calendar": {
          "type": "string",
          "enum": [
            "sport",
            "shopping",
            "home",
            "working"
          ]
        },
    "command_thought": {
          "type": "string",
          "enum": ["move", "stop", "right", "left"]
        }
  },
  "required": ["uuid", "features", "calendar", "command_thought"]
}